  "description": "The graduation backend project 2024 | SuezCanalUniversity ",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production nodemon server.js",
    "start": "node server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:prod": "NODE_ENV=production npx sequelize-cli db:migrate",
    "migrate:create": "npx sequelize-cli migration:create --name",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:prod": "NODE_ENV=production npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
    "migrate:undo:all:prod": "NODE_ENV=production npx sequelize-cli db:migrate:undo:all"
  },
